{
  "name": "automated-qa-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "FRONTEND": "",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "": "",
    "LOCAL": "",
    "open": "npx cypress open -C cypress.open.config.js",
    "feature": "npx cypress run --browser chrome -C cypress.feature.config.js",
    "cy": "npx cypress run --browser chrome -C cypress.cy.config.js && npm run merge-mochawesome && npm run json2html",
    "component:open": "npx cypress open --browser chrome -C cypress.comp.config.js",
    "component": "npx cypress run --component -C cypress.comp.config.js",
    "CI": "",
    "ga:feature": "npm run prereport && cypress run --browser chrome -C cypress.feature.config.js",
    "ga:feature:record": "npm run prereport && cypress run --browser chrome -C cypress.feature.config.js -e TAGS='@regression' --record",
    "ga:cy": "npm run prereport && cypress run --browser chrome -C cypress.cy.config.js && npm run merge-mochawesome && npm run json2html",
    "ga:component": "npm run prereport && cypress run --component -C cypress.comp.config.js",
    "ga:performance": "npm run prereport && cypress run --browser chrome -C cypress.performance.config.js --spec cypress/**/lighthouse.cy.js",
    "ga:visual": "npm run prereport && cypress run --browser chrome -C cypress.visual.config.js --spec cypress/**/visual.cy.js",
    "PRE-REPORT CLEANUP": "",
    "delete:reports": "rm -rf cypress/reports/* || true",
    "delete:results": "rm -rf cypress/results/* || true",
    "prereport": "npm run delete:reports && npm run delete:results",
    "REPORT CREATION": "",
    "merge-mochawesome": "npx mochawesome-merge 'cypress/results/*.json' > cypress/reports/mochawesome.json",
    "json2html": "npx marge cypress/reports/mochawesome.json --reportDir cypress/reports/",
    "postreport": "rm -rf cypress/results",
    "html-report": "node ./util/cucumber-html-report.js",
    "combine:reports": "jrm cypress/reports/combined-report.xml \"cypress/reports/*.xml\"",
    "GIT HOOKS & LINTING": "",
    "prepare": "cd .. && husky install",
    "cy:eslint": "npx eslint cypress/**/*.js",
    "cy:gherkin-lint": "gherkin-lint -c .gherkin-lintrc",
    "cy:prettier": "npx prettier --write cypress/**/*.js",
    "cy:lint-jobs": "npm run cy:prettier && npm run cy:eslint && npm run cy:gherkin-lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress-audit/lighthouse": "^1.4.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "axe-core": "^4.8.2",
    "chai-sorted": "^0.2.0",
    "cypress": "^13.3.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-image-diff-js": "^1.30.1",
    "cypress-mochawesome-reporter": "^3.6.0",
    "cypress-real-events": "^1.10.3",
    "eslint": "^8.50.0",
    "eslint-plugin-cypress": "^2.15.1",
    "gherkin-lint": "^4.2.2",
    "husky": "^8.0.3",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/step_definitions/*.js",
      "cypress/e2e/step_definitions/**/*.js"
    ],
    "html": {
      "enabled": true,
      "output": "cypress/reports/report.html"
    },
    "json": {
      "enabled": false,
      "output": "cypress/reports/cucumber-report.json"
    },
    "pretty": {
      "enabled": true
    }
  }
}
